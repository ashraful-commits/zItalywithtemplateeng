<!--////////////////////////////////////Container-->
<section id="container" class="sub-page">
	<div class="wrap-container zerogrid">
		<div class="crumbs">
			<ul>
				<li><a href="home">Home</a></li>
				<li><a href="menu">Menu</a></li>
			</ul>
		</div>
		<div id="main-content">
			<div class="wrap-content">
				<div class="row">
					<div class="col-1-3">
						<div class="wrap-col">
							<h3>Pasta Plates</h3>
							<% pasta.forEach(element => { %>
								<div class="post">
									<a href="#"><img src="<%= element.img %> "/></a>
									<div class="wrapper">
									  <h5><a href="#"><%= element.food_name %> </a></h5>
									  <span>$25 - $26</span>
									</div>
								</div>
							<% }) %>
						
						</div>
					</div>
					<div class="col-1-3">
						<div class="wrap-col">
							<h3>Starters</h3>
							<% starters.forEach(element => { %>
								<div class="post">
									<a href="#"><img src="<%= element.img %> "/></a>
									<div class="wrapper">
									  <h5><a href="#"><%= element.food_name %> </a></h5>
									   <span>$25 - $26</span>
									</div>
								</div>
							<% }) %>
						
						</div>
					</div>
					<div class="col-1-3">
						<div class="wrap-col">
							<h3>Salads</h3>
							<% salads.forEach(element => { %>
								<div class="post">
									<a href="#"><img src="<%= element.img %> "/></a>
									<div class="wrapper">
									  <h5><a href="#"><%= element.food_name %> </a></h5>
									  <span>$25 - $26</span>
									</div>
								</div>
							<% }) %>
							
					</div>
				</div>
				<div class="row">
					<div class="col-1-3">
						<div class="wrap-col">
							<h3>Chef's Specials</h3>
							<% chefspacial.forEach(element => { %>
								<div class="post">
									<a href="#"><img src="<%= element.img %> "/></a>
									<div class="wrapper">
									  <h5><a href="#"><%= element.food_name %> </a></h5>
									   <span>$25 - $26</span>
									</div>
								</div>
							
							<% }) %>
						</div>
					</div>
					<div class="col-1-3">
						<div class="wrap-col">
							<h3>Fast Food</h3>
							<% fastFood.forEach(element => { %>
								<div class="post">
									<a href="#"><img src="<%= element.img %> "/></a>
									<div class="wrapper">
									  <h5><a href="#"><%= element.food_name %> </a></h5>
									  <span>$25 - $26</span>
									</div>
								</div>
							
							<% }) %>
						</div>
					</div>
					<div class="col-1-3">
						<div class="wrap-col">
							<h3>Seafood Specials</h3>
							<% seafood.forEach(element => { %>
								<div class="post">
									<a href="#"><img src="<%= element.img %> "/></a>
									<div class="wrapper">
									  <h5><a href="#"><%= element.food_name %> </a></h5>
									   <span>$25 - $26</span>
									</div>
								</div>
								
							<% }) %>
						</div>
					</div>
				</div>
			</div>
		</div> 
	</div>
</section>

